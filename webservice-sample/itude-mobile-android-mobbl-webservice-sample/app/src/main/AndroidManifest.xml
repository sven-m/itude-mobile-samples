<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="0.0.0.1" package="com.itude.mobile.mobbl.samples.webservice">
    <uses-sdk android:minSdkVersion="9" android:targetSdkVersion="11"/>
	<uses-permission android:name="android.permission.INTERNET" />
	
    <application android:label="MobblWebServiceSample" android:name="com.itude.mobile.mobbl.samples.masterdetail.WebserviceApplicationCore" android:theme="@style/Theme.AppCompat.Light">
    
        
		<activity android:name="com.itude.mobile.mobbl.core.MBSplashScreen" android:noHistory="true" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
           <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="orientation" android:name="com.itude.mobile.mobbl.core.controller.MBViewManager"/>
        <activity android:configChanges="orientation" android:name="com.itude.mobile.mobbl.core.controller.MBPhoneViewManager"/>
        <activity android:configChanges="orientation" android:name="com.itude.mobile.mobbl.core.controller.MBTabletViewManager"/>
		<activity android:configChanges="orientation" android:name="com.itude.mobile.mobbl.core.controller.MBDialogController"/>
		<activity android:name="com.itude.mobile.mobbl.core.controller.util.MBBasicViewController"/>
    </application>
</manifest>